{
  "name": "unified-platform",
  "version": "0.1.0",
  "description": "A comprehensive platform combining social networking, e-commerce, e-learning, blogging, and job marketplace features with integrated multi-model AI capabilities",
  "private": true,
  "scripts": {
    "dev:frontend": "cd private/src/frontend && next dev",
    "dev:backend": "cd private/src/backend && node server.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "cd private/src/frontend && next build",
    "start": "cd private/src/frontend && next start",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2",
    "mongoose": "^7.4.3",
    "pg": "^8.11.3",
    "prisma": "^5.1.1",
    "jsonwebtoken": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2",
    "axios": "^1.4.0",
    "react-query": "^3.39.3",
    "redux": "^4.2.1",
    "react-redux": "^8.1.2",
    "@reduxjs/toolkit": "^1.9.5",
    "formik": "^2.4.3",
    "yup": "^1.2.0",
    "styled-components": "^6.0.7",
    "tailwindcss": "^3.3.3",
    "@tensorflow/tfjs": "^4.10.0",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "@types/react": "^18.2.20",
    "@types/node": "^20.5.0",
    "@types/express": "^4.17.17",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "jest": "^29.6.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "supertest": "^6.3.3",
    "prettier": "^3.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
